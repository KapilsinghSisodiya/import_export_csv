<h1>User#index</h1>
<p>Find me in app/views/user/index.html.erb</p>
<%= link_to "import", import_users_path %>
<%= link_to "export", export_users_path(format: "csv") %>

<table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Telephone Number</th>
        <th>Website</th>
      </tr>
    </thead>
    <tbody>
			<%@users.each do |u|%>
			<tr>
			<td><%=  u.name %></td>
			<td><%=  u.email %></td>
			<td><%=  u.telephone_number %></td>
			<td><%=  u.website %></td>
			</tr>
			<%end%>
    </tbody>
  </table>